<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kbc21.guideditor" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="cordova-plugin-mifare-ultralight">
        <param name="android-package" value="fi.roopehakulinen.CordovaPluginMifareUltralight.CordovaPluginMifareUltralight" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>GUIDEdit</name>
    <description>
        A sample application which can rewrite NFC tags utilising the Ultimaker custom format.
    </description>
    <author email="thorntos@lancaster.ac.uk" href="https://www.lancaster.ac.uk/infolab21">
        thorntos
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="cdv://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
